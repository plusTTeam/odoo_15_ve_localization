<odoo>
    <record id="lang_es_ve" model="ir.model.data">
        <field name="name">lang_es_VE</field>
        <field name="noupdate">True</field>
        <field name="module">l10n_ve_plusteam</field>
        <field name="model">res.lang</field>
        <field name="res_id" model="res.lang" search="[('code', '=', 'es_VE')]"/>
    </record>
    <record model="res.lang" id="base.lang_es_VE">
        <field name="active" eval="True"/>
    </record>
    <function name="switch_lang" model="base.language.install"/>
</odoo>

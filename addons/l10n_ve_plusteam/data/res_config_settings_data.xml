<?xml version="1.0" ?>
<odoo>
    <data noupdate="1">
        <record id="settings" model="res.config.settings">
            <field name="chart_template_id" ref="ve_chart_template_amd"/>
            <field name="account_tax_fiscal_country_id" ref="base.ve"/>
            <field name="currency_id" ref="base.VEF"/>
            <field name="withholding_journal_id" ref="journal_withholding"/>
        </record>
        <function model="res.config.settings" name="execute">
            <value model="res.config.settings" search="[('id', '=', ref('settings'))]"/>
        </function>
    </data>
</odoo>
